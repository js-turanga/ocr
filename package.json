{
    "name": "@turanga/ocr",
    "version": "0.1.0",
    "description": "Optical Character Recognizen based on tesseract",
    "main": "dist/ocr.js",
    "scripts": {
        "eslint": "npm run eslint:js && npm run eslint:md",
        "eslint:js": "eslint src/ test/ --fix",
        "eslint:md": "eslint --fix --ext md README.md --rule strict:off --rule no-undef:off --rule no-unused-vars:off --rule func-names:off --rule no-console:off --rule consistent-return:off --rule import/no-unresolved:off --rule import/extensions:off --parser-options sourceType:module",
        "test": "mocha --reporter list",
        "bundle.dev": "webpack --mode development",
        "bundle.prod": "webpack --mode production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/js-turanga/ocr.git"
    },
    "keywords": [
        "ocr"
    ],
    "author": "js-turanga <js.turanga@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/js-turanga/pipeline/issues"
    },
    "homepage": "https://github.com/js-turanga/pipeline#readme",
    "dependencies": {
        "tesseract.js": "^2.1.5"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.2.3",
        "chai": "*",
        "eslint": "^8.1.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-markdown": "^2.2.1",
        "eslint-plugin-mocha": "^9.0.0",
        "fs": "^0.0.1-security",
        "mocha": "^9.1.3",
        "webpack": "^5.62.1",
        "webpack-cli": "^4.9.1"
    }
}
